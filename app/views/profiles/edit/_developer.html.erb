<h1>Edit Developer</h1>

<%= simple_form_for(@profile, as: 'profile', url: profile_path, html: {class:"custom"}) do |f| %>
  <div class="form-inputs">
    <%= f.input :relocate %>
    <%= f.input :position %>
    <%= f.input :location %>
  </div>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>